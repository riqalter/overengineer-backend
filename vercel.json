{
    "version": 2,
    "builds": [
        {
            "buildCommand": "prisma generate && prisma migrate deploy",
            "src": "api/index.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/index.ts"
        }
    ]
}