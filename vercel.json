{
    "version": 2,
    "builds": [
        {
            "buildCommand": "prisma generate && prisma migrate deploy && tsc",
            "src": "dist/app.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/app.js"
        }
    ]
}