{
    "version": 2,
    "buildCommand": "prisma generate && prisma migrate deploy && tsc",
    "builds": [
        {
            "src": "dist/api.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/api.js"
        }
    ]
}